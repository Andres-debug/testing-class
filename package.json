{
  "name": "testing-class",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:integration": "jest integration-tests --verbose",
    "test:incremental": "jest integration-tests/tests/incremental.test.js",
    "test:bigbang": "jest integration-tests/tests/bigbang.test.js",
    "test:drivers-stubs": "jest integration-tests/tests/drivers-stubs.test.js",
    "test:unit": "jest unit-test",
    "test:all": "jest --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "jest": "^30.1.2"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "integration-tests/**/*.js",
      "unit-test/**/*.js",
      "!**/*.test.js",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "verbose": true,
    "testTimeout": 15000,
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"]
  }
}
