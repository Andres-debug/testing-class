{
  "name": "testing-class",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:integration": "jest integration-tests --verbose",
    "test:incremental": "jest integration-tests/tests/incremental.test.js",
    "test:bigbang": "jest integration-tests/tests/bigbang.test.js",
    "test:drivers-stubs": "jest integration-tests/tests/drivers-stubs.test.js",
    "test:unit": "jest unit-test",
    "test:react": "jest react-components --verbose",
    "test:react:watch": "jest react-components --watch",
    "test:all": "jest --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.2.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "integration-tests/**/*.js",
      "unit-test/**/*.js",
      "react-components/**/*.js",
      "react-components/**/*.jsx",
      "!**/*.test.js",
      "!**/*.test.jsx",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/*.test.js",
      "**/*.test.jsx"
    ],
    "verbose": true,
    "testTimeout": 15000
  }
}
